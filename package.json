{
  "name": "evofithealthprotocol",
  "version": "1.0.0",
  "description": "EvoFitHealthProtocol - Advanced Health Protocol Management System with PDF Export",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "docker-compose --profile dev up -d",
    "stop": "docker-compose --profile dev down",
    "logs": "docker logs evofithealthprotocol-dev -f",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "tsc -p server/tsconfig.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:run && npm run test:e2e",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
